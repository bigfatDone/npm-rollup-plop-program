{
  "name": "zys-npm-rollup-plop-program",
  "version": "0.0.3",
  "private": false,
  "description": "基于vue-cli搭建npm插件",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-bundle": "vue-cli-service build --target lib --name zys-compnonent ./packages/index/index.js",
    "build:components": "rollup --config ./build/rollup.config.js",
    "plop": "plop start",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/zys-compnonent.common.js",
  "files": [
    "dist/*",
    "es/*",
    "lib/*",
    "packages/*",
    "src/*",
    "package.json",
    "README.md"
  ],
  "author": "bigfatdone",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "zys-npm-rollup-plop-program": "0.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "lerna": "4.0.0",
    "plop": "^2.7.4",
    "rollup": "^2.58.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-vue": "5.0.1",
    "vue-loader": "15.9.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
